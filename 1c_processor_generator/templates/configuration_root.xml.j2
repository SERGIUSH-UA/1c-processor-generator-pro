				<xr:ClassId>e3687481-0a87-462c-a166-9f34594f9bba</xr:ClassId>
				<xr:ObjectId>{{ internal_object_id_3 }}</xr:ObjectId>
			</xr:ContainedObject>
			<xr:ContainedObject>
