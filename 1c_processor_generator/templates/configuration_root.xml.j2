{%- if platform_version_tuple >= (8, 3, 20) %}
			<DatabaseTablespacesUseMode>DontUse</DatabaseTablespacesUseMode>
{%- endif %}
			<CompatibilityMode>{{ compatibility_mode }}</CompatibilityMode>
			<DefaultConstantsForm/>
		</Properties>
		<ChildObjects>
			<Language>Русский</Language>
			<Language>English</Language>
{%- for picture_name in common_picture_names %}
			<CommonPicture>{{ picture_name }}</CommonPicture>
{%- endfor %}
{%- for module_name in common_module_names %}
			<CommonModule>{{ module_name }}</CommonModule>
{%- endfor %}
{%- for catalog_name in catalog_names %}
			<Catalog>{{ catalog_name }}</Catalog>
{%- endfor %}
{%- for document_name in document_names %}
			<Document>{{ document_name }}</Document>
{%- endfor %}
{%- if include_processor %}
			<DataProcessor>{{ processor_name }}</DataProcessor>
{%- endif %}
		</ChildObjects>
	</Configuration>
